{
    "env": {
        "commonjs": true,
        "es2021": true,
        "browser": true
    },
    "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
    "plugins": ["prettier"],
    "overrides": [],
    "parserOptions": {
        "project": "./tsconfig.eslint.json"
    },
    "rules": {
        "prettier/prettier": ["warn"],
        "no-console": "off",
        "no-plusplus": "off"
    }
}
